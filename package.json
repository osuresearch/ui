{
  "name": "@osuresearch/ui",
  "version": "5.0.0-alpha.23",
  "license": "MIT",
  "description": "React UI components for OSU Office of Research applications",
  "keywords": [
    "ohio-state-university"
  ],
  "homepage": "https://github.com/osuresearch/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/osuresearch/ui"
  },
  "bugs": {
    "url": "https://github.com/osuresearch/ui/issues"
  },
  "author": {
    "name": "Chase McManning",
    "url": "https://github.com/McManning"
  },
  "sideEffects": false,
  "main": "dist/index.umd.js",
  "module": "dist/index.module.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "pretest": "yarn lint",
    "test": "jest",
    "build": "microbundle --jsx React.createElement --jsxFragment React.Fragment --visualize",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "lint": "eslint . --cache --ext ts,tsx --fix",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build -o docs-build",
    "generate:component": "node ./scripts/makeComponent.mjs"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@mui/material": "^5.14.1",
    "react": "^16.8.0 || ^17.0.0-rc.1 || ^18.0.0",
    "react-hook-form": "^7.0"
  },
  "peerDependenciesMeta": {
    "react-hook-form": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@emotion/react": "^11.11.1",
    "@iconify/icons-mdi": "^1.2.44",
    "@mdx-js/react": "^2.3.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-a11y": "^7.1.0",
    "@storybook/addon-actions": "^7.1.0",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/blocks": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-webpack5": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "inquirer": "^9.2.8",
    "jest": "^29.4.0",
    "jest-environment-jsdom": "^29.4.0",
    "lint-staged": "^13.1.0",
    "microbundle": "^0.15.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "storybook": "^7.1.0",
    "storybook-dark-mode": "^3.0.0",
    "ts-jest": "^29.0.5",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "mrmConfig": {
    "defaultBranch": "main",
    "contributionPolicy": "internal",
    "dataClassification": "public",
    "securityContact": "ordevelopment@osu.edu",
    "conductContact": "ordevelopment@osu.edu"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jest-environment-jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run lint"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/nunito-sans": "^5.0.6",
    "@fontsource/source-serif-pro": "^5.0.5",
    "@iconify/react": "^4.1.0",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.1",
    "@mui/x-date-pickers": "^6.10.1",
    "@osuresearch/iconography": "^1.1.0",
    "@osuresearch/types": "^1.0.3",
    "@react-types/shared": "^3.18.1",
    "autosuggest-highlight": "^3.3.4",
    "dayjs": "^1.11.9",
    "react-stately": "^3.24.0",
    "react-use": "^17.4.0"
  }
}
